@model UserDeleteViewModel

<input type="hidden" asp-for="Id" />
<div class="form-group">
    <label class="control-label col-sm-2" for="Username">Username</label><div class="col-sm-10">
        <input class="form-control disabled text-box single-line" disabled id="Username" name="Username" type="text" value="@Model.Username">
        <span class="field-validation-valid text-danger" data-valmsg-for="Username" data-valmsg-replace="true">
        </span>
    </div>
</div>
<div class="form-group">
    <label class="control-label col-sm-2" for="Email">Email</label><div class="col-sm-10">
        <input class="form-control disabled text-box single-line" disabled id="Email" name="Email" type="text" value="@Model.Email">
        <span class="field-validation-valid text-danger" data-valmsg-for="Email" data-valmsg-replace="true"></span>
    </div>
</div>
<div class="form-group">
    <label class="control-label col-sm-2" for="FullName">FullName</label>
    <div class="col-sm-10">
        <input class="form-control disabled text-box single-line" disabled id="FullName" name="FullName" type="text" value="@Model.FullName">
        <span class="field-validation-valid text-danger" data-valmsg-for="FullName" data-valmsg-replace="true"></span>
    </div>
</div>
<div class="form-group">
    <label class="control-label col-sm-2" for="DateOfRegistry">Date Of Registry</label>
    <div class="col-sm-10">
        <input class="form-control disabled text-box single-line" disabled id="DateOfRegistry" name="DateOfRegistry" type="text" value="@Model.DateOfRegistry">
        <span class="field-validation-valid text-danger" data-valmsg-for="DateOfRegistry" data-valmsg-replace="true"></span>
    </div>
</div>